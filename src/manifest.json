{
    "manifest_version": 2,
    "author": "fedemozzon",
    "name": "WebExtension",
    "version": "1.0",
    "content_scripts": [{
      "matches": ["*://*/*"],
      "js": [ "content_scripts/main.js" ]
      }],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://stackpath.bootstrapcdn.com; object-src 'self';",
  "browser_action": {
    "default_title": "Make this page red"
  }
}
